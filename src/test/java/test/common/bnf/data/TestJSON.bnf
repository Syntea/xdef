boolean ::= "true" | "false"number ::= int frac exp | int frac | int exp | intint ::= "-"? digitsdigits ::= [0-9]+frac ::= "." digitsexp ::= e digitse ::= "e+" | "e-" | "E+" | "E-" | "e" | "E"string ::= ('"' ([^\"] | control-character)* '"' |  "'" ([^\'] | control-character)* "'")control-character ::= '\"' | '\\' | '\/' | '\b' | '\f' | '\n' | '\r' | '\t' |  ('\u' [0-9a-fA-F]{4})S ::= [#9#10#13 ]* /*skipped white spaces*/object ::= "{" (member)* S "}"member ::= S pair (S "," S pair)*pair ::= string S (":" | "=>") S valuearray ::= ("[" S element? S "]") | ("array" S "(" S element? S ")")element ::= value (S "," S value )*value ::= pair | "null" | boolean | number | string | array | object