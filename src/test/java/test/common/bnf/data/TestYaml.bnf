boolean ::= "true" | "false"number ::= int frac exp | int frac | int exp | intint ::= "-"? digitsdigits ::= [0-9]+frac ::= "." digitsexp ::= e digitse ::= "e" | "E" ("+" | "-")?string ::= ('"' ([^\"] | control-character)* '"' |  "'" ([^\'] | control-character)* "'")control-character ::= '\"' | '\\' | '\/' | '\b' | '\f' | '\n' | '\r' | '\t' |  ('\u' [0-9a-fA-F]{4})date ::= int"-" [0-9]{1,2} "-" [0-9]{1,2} ("T"[0-9]{1,2} ":"T"[0-9]{1,2} (":" [0-9]{1,2} ("." [0-9]+)? )?) ? ([-+][0-9]{2} ":" [0-9]{2} )?S ::= [#9#10#13 ]* /*skipped white spaces*/document S "---" member*  S "..."