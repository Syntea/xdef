<?xml version="1.0" encoding="UTF-8"?>
<xd:def xmlns:xd="http://www.syntea.cz/xdef/2.0"
  root="SouborD5"
  name="SouborD5">
  <Get_AllPDN KodUtvaruPolicie="optional num(6)"
    RokDN="optional ISOyear()"
    CisloDN="optional num(5)"
    CJ="optional string(1,50)"
    PlatnostOd="optional datetime('dd.MM.yyyy')"/>
  <Get_ListPDN xd:script="occurs 0..1"
    DatumCasDNOd="optional datetime('dd.MM.yyyy')"
    DatumCasDNDo="optional datetime('dd.MM.yyyy')"
    Okres="optional string()"
    Obec="optional string(1,50)">
    <Subjekt xd:script="occurs 0..1"
      Jmeno="optional string(1,24)"
      Prijmeni="optional string(1,36)"
      RC="optional num(9,10)"
      DatumNarozeni="optional datetime('dd.MM.yyyy')"
      StatPrislusnost="optional string(1,3)"
      NazevFirmy="optional string(1,100)"
      IC="optional int(1,99999999)"/>
    <Vozidlo xd:script="occurs 0..1"
      SPZ="optional string(1,20)"
      MPZ="optional string()"
      VIN="optional string(1,26)"
      CisloZK="optional string(1,40)"/>
  </Get_ListPDN>
  <Get_PDN KodUtvaruPolicie="optional num(6)"
    RokDN="optional ISOyear()"
    CisloDN="optional num(5)"
    CJ="optional string(1,50)"
    PlatnostOd="optional datetime('dd.MM.yyyy')"/>
  <SouborD5 KodUtvaruPolicie="optional num(6)"
    RokDN="optional ISOyear()"
    CisloDN="optional num(5)"
    CJ="optional string(1,50)"
    PlatnostOd="optional datetime('dd.MM.yyyy')"
    Poradi="required int(1,99999)"/>
</xd:def>