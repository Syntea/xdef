Char         ::= $UTFCharS            ::= [#9#10#13 ]+ /*whitespace*/HexDigit     ::= [0-9] | [a-fA-F]Letter       ::= $letter/* Simple values */Null         ::= 'null'Boolean      ::= 'true' | 'false'/* String */AnyChar      ::= Char - '\' - '"'UTFSpec      ::= '\u' HexDigit {4} /*hexadecimal specification of char*/StringPart   ::= '\\' | '\"' | '\n' | '\r' | '\t' | '\f' | '\b'             | UTFSpec | AnyCharString       ::= '"' StringPart* '"'/* Number */Digits       ::= [0-9] [0-9]*SignedInteger::= '-'? ('0' | [1-9] [0-9]*)Exponent     ::= ('e' | 'E') ('+' | '-')? DigitsFloatPart    ::= '.' Digits Exponent? | ExponentFloatNumber  ::= SignedInteger FloatPartInteger      ::= SignedIntegerNumber       ::= FloatNumber | SignedInteger | 'NaN' | '-' ? 'INF'/* Complex values */SimpleValue  ::= S? (Null | Boolean | String | Number)Identifier   ::= ('_' | Letter) ([_0-9] | Letter)*NamedValue   ::= S? String S? ':' JsonValueArray        ::= S? '[' (JsonValue (S? ',' JsonValue)*)? S? ']'Map          ::= S? '{' (NamedValue (S? ',' NamedValue)*)?  S? '}'JsonValue    ::= (Array | Map | SimpleValue)json         ::= JsonValue S?