        <ObjStranka xd:script="occurs 1; ref ZaznamPDN_ObjStrankaDN_type_cType"/>
        <Vozidlo xd:script="occurs 0..*; ref ZaznamPDN_VozidloDN_type_cType"/>
        <Tramvaj xd:script="occurs 0..*; ref ZaznamPDN_TramvajDN_type_cType"/>
