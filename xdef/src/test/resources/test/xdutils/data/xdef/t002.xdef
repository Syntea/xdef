<xd:def xmlns:xd="http://www.syntea.cz/xdef/3.1" name="t002" root="a">
  <xd:declaration>
    type a_Type{parse: string(1, 8);}
  </xd:declaration>
  <xd:declaration>
    type b_Type{parse: string(1, 8);}
  </xd:declaration>
  <a a="optional a_Type()">
    <xd:sequence xd:script="occurs 1">
      <b xd:script="occurs 1"
        b="optional b_Type()">
        optional int()
      </b>
    </xd:sequence>
  </a>
</xd:def>