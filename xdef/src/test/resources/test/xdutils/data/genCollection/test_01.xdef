<xd:def xmlns:xd      =  "http://www.syntea.cz/xdef/2.0"
        name      =  "main"
        root      =  "Root" >

    <Root>
        <xd:mixed>
            <Test01>int</Test01>
            <Test02>int()</Test02>
            <Test03>int(%minInclusive=0, %maxInclusive=100)</Test03>
            <Test04>list(%item=int)</Test04>
            <Test05>list(%item=int())</Test05>
            <Test06>list(%item=int(%minInclusive=0, %maxInclusive=100), %length=3)</Test06>
            <Test08>union(%item=[int, string])</Test08>
            <Test09>union(%item=[int(), string(%minLength=1, %maxLength=30)])</Test09>
            <Test10>union(%item=[int(%minInclusive=0, %maxInclusive=100), string(%minLength=1, %maxLength=30)])</Test10>
            <Test11>union(%item=[list(%item=int(%minInclusive=0, %maxInclusive=100), %length=3), string(%minLength=1, %maxLength=30)])</Test11>
        </xd:mixed>
    </Root>

</xd:def>