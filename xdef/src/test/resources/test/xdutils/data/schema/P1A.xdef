  <xdef:def xmlns:xdef="http://www.syntea.cz/xdef/2.0"
            xdef:classes="cz.syntea.ckp.vr.ivr.P1A_ChkParser"
            xdef:script="options setAttrUpperCase,ignoreEntities"
            impl-version="2.4.0.2"
            impl-date="06.04.2007"
            root="OpravaXPolPS"
            name="OpravaXPolPS">
    <OpravaXPolPS xdef:script="finally outputIVR('OpravaXPolPS_')"
                  IdentZaznamu="required string(1,12);                                                onFalse setErr(4208); onAbsence setErr(4202)"
                  CisloSmlouvy="required string(1,35);                                                onFalse setErr(4208); onAbsence setErr(4202)"
                  PoradiVozidla="required num(1,10);                                                   onFalse setErr(4208); onAbsence setErr(4202)"
                  CisloDokladuPojisteni="optional string(1,35);                                                onFalse setErr(4208)"
                  NositelPSP="optional enum('S','V');                                               onFalse setErr(4225)"
                  DatumUcinnosti="optional datetime('yyyyMMdd|dd.MM.yyyy','yyyyMMdd');           onTrue dateDavka(4221);                onFalse setErr(4208)"
                  DatumUzavreni="optional datetime('yyyyMMdd|dd.MM.yyyy','yyyyMMdd');           onTrue dateDavka(4221);                onFalse setErr(4208)"
                  DatumUkonceni="optional datetime('yyyyMMdd|dd.MM.yyyy','yyyyMMdd');           onTrue dateDavka(4221);                onFalse setErr(4208)"
                  DuvodZanikuSmlouvy="optional tab('PS_DuvodZanikuSmlouvy','KodDuvoduZanikuSmlouvy');       onFalse setErr(4225)">
      <Pojistnik xdef:script="occurs 0..1; ref P1_common#Subjekt"/>
      <Provozovatel xdef:script="occurs 0..1; ref P1_common#Subjekt"/>
      <Vlastnik xdef:script="occurs 0..1; ref P1_common#Subjekt"/>
      <VozidloSmlouvy xdef:script="occurs 0..1"
                      PlatnostOd="optional;  onTrue setErr(4204)"
                      TarifniSkupina="optional tab('CC_TarifniSkupinaVozidel','KodTarifniSkupinyVozidel'); onFalse setErr(4225)"
                      LimitPlneniMajetek="optional int(1,999_999_999); onTrue {if (parseInt(getText()) &lt; 18_000_000) setErr(4206);} onFalse setErr(4208)"
                      LimitPlneniZdravi="optional int(1,999_999_999); onTrue {if (parseInt(getText()) &lt; 35_000_000) setErr(4206);} onFalse setErr(4208)"
                      Leasing="optional enum('A','N');                                              onFalse setErr(4225)"
                      Pojistne="optional int(1,999_999);                                             onFalse setErr(4208)"
                      PojistneBM="optional int(1,999_999);                                             onFalse setErr(4208)">
        <Vozidlo xdef:script="occurs 0..1"
                 CisloTP="optional pic('AA999999');                                            onFalse setErr(4208)"
                 VIN="optional string(1,26);                                               onFalse setErr(4208)"
                 SPZ="optional string(2,12);                                               onFalse setErr(4208)"
                 DruhVozidla="optional fil0(2) AND tab('CC_DruhVozidla','KodDruhuVozidla');        onFalse setErr(4225)"
