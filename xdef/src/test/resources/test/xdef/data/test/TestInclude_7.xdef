<xd:def
   xmlns:xd     = "http://www.syntea.cz/xdef/3.1"
   xmlns:ws     = "http://schema.xmlsoap.org/wsdl/"
   xd:name      = "wsdefinitions"
   xd:root      = "ws:definitions"
   xd:script    = "options ignoreEntities"
   impl-version = "2.5.0.2"
   impl-date    = "31.05.2007">

 <ws:definitions              xd:script="init {;}"
          xmlns:vp            ="http://docs.oasis-open.org/wsbpel/2.0/varprop"
          xmlns:pl            ="http://docs.oasis-open.org/wsbpel/2.0/plnktype"
          xmlns:sp            ="http://schemas.xmlsoap.org/soap/http"
          xmlns:wfsq          ="http://www.syntea.cz/xbpel/GAMqueue/2007/07"
          targetNamespace     ="required string()"
          name                ="required string()">

   <xi:include xmlns:xi="http://www.w3.org/2001/XInclude"
               href="xxx.xxx"/>
   <xi:include xmlns:xi="http://www.w3.org/2001/XInclude"
               href="yyy.yyy"/>
 </ws:definitions>

</xd:def>