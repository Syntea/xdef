<xd:def xmlns:xd='http://www.xdef.org/xdef/4.2' name='Ab' root='A' >
<xd:declaration scope='global'>
  type names string(1,100);
  type name names;
  type addr string(1,100);
  type city string(1,80);
  type country string(1,80);
  type text string(1,1000);
  type quantity int(1,*);
  type price decimal(0,*);
</xd:declaration>
<A orderid="num(2,10)">
 <orderperson>name</orderperson>
  <shipto>
    <name>name</name>
    <address>addr</address>
    <city>city</city>
    <country>country</country>
  </shipto>
  <item xd:script='+'>
    <title>text</title>
    <note xd:script='?'>? text</note>
    <quantity>int(1,*)</quantity>
    <price>price</price>
  </item>
</A>
</xd:def>
