<?xml version="1.0" encoding="utf-8"?>
<xd:def  xmlns:xd      ="http://www.xdef.org/xdef/4.0"
         impl-version  ="2023/10.1" impl-date ="2023-10-31"
         xd:name       ="D3_common" xd:root="SouborD3A">

    <xd:declaration scope="global">
        type  xsDateTime              xdatetime('yyyy-MM-ddTHH:mm:ss|'+
        '{HH00mm00ss00}yyyy-MM-dd[THH:mm[:ss][Z]]|'+
        '{HH00mm00ss00}yyyyMMdd[HHmm[ss]]|'+
        '{HH00mm00ss00}d.M.yyyy[ HH:mm[:ss]]', 'yyyy-MM-ddTHH:mm:ss');
        type  channelTypeVersion      string(7,8);
        type  batchSeq                long(1,99_999_999);
        type  xsDateTimeOd            xdatetime('yyyy-MM-ddTHH:mm:ss|'+
        '{HH00mm00ss00}yyyy-MM-dd[THH:mm[:ss][Z]]|'+
        '{HH00mm00ss00}yyyyMMdd[HHmm[ss]]|'+
        '{HH00mm00ss00}d.M.yyyy[ HH:mm[:ss]]', 'yyyy-MM-ddTHH:mm:ss');
        type  xsDateTimeDo            xdatetime('yyyy-MM-ddTHH:mm:ss|'+
        '{HH23mm59ss59}yyyy-MM-dd[THH:mm[:ss][Z]]|'+
        '{HH23mm59ss59}yyyyMMdd[HHmm[ss]]|'+
        '{HH23mm59ss59}d.M.yyyy[ HH:mm[:ss]]', 'yyyy-MM-ddTHH:mm:ss');
        type  fileSeq                 long(1,9_999);
        type  recordSeq               long(1,9_999_999_999);
        type  md5                     string(32);
        type  poradiVozidla           string(1,10);
        type  cisloDokladuPojisteni   string(1,35);
        type  cisloSmlouvy            string(1,35);
        type  cisloDokladu            string(1,12);  	/* Číslo dokladu, číslo TP, číslo ORV, ... */
        type  alkoholDechDN           string(1,18);
        type  alkoholKrevDN           string(1,11);
        type  castkaKcDN              long(0,999_999_999);
        type  cisloDN                 num(7);  	         /* Číslo dopravní nehody */
        type  cisloDokladuDN          string(1,36); 	/* Číslo doklady v DN */
        type  cisloDokladuPojisteniDN string(1,35);
        type  cisloDomuDN             string(1,10);       /* Číslo domu, popř. včetně označení vchodu */
        type  cisloZKDN               string(1,40);       /* Číslo ZK v DN */
        type  cjRelacePolicieDN       string(1,50);
        type  datumCasDN              xdatetime('yyyyMMddHHmm[ss]|'+
                                                'yyyy-MM-dd[THH:mm[:ss][ZZZZZZ]]|'+
                                                'd.M.yyyy[ HH:mm[:ss]]|'+
                                                'd.M.yy[ HH:mm[:ss]]','dd.MM.yyyy HH:mm'); /* format pouzivany v  */
                                                                                           /* souborovem rozhrani */
        type  datumDN                 xdatetime('yyyyMMdd|'+
                                        'yyyy-MM-dd[Z]|'+
                                        'd.M.yyyy|'+
                                        'dd.MM.yy','dd.MM.yyyy');                  /* format pouzivany v */
                                                                                   /* souborovem rozhrani */
        type  datumCasWSDN            xdatetime('yyyyMMddHHmm[ss]|'+
                                                'yyyy-MM-dd[THH:mm[:ss][ZZZZZZ]]|'+
                                                'd.M.yyyy[ HH:mm[:ss]]|'+
                                                'd.M.yy[ HH:mm[:ss]]','yyyy-MM-ddTHH:mm:ss');/* format pouzivany ve */
                                                                                             /* webovem rozhrani    */
        type  datumWSDN               xdatetime('yyyyMMdd|'+
                                                'yyyy-MM-dd[Z]|'+
                                                'd.M.yyyy|'+
                                                'dd.MM.yy','yyyy-MM-dd'); /* format pouzivany ve */
                                                                          /* webovem rozhrani    */
        type  delkaPrilohyDN          long(0,9_999_999_999); /* Délka přílohy v DN */
        type  detailFaktorDN          string(1,2048);        /* Faktor popis v klasifikaci DN */
        type  druhPovozuDN            string(1,30);         /* DN */
        type  druhPredmetuDN          string(1,255);        /* DN */
        type  druhSoupravyDN          enum('vlak');         /* DN */
        type  druhVozidlaDN           string(1,30);         /* DN */
        type  druhZvireteDN           string(1,255);        /* DN */
        type  duvodOdeslaniDN         string(1,34);         /* DN */
        type  evidCisloDN             string(1,20);         /* DN */
        type  formatSouboruDN         enum('jpg','pdf','JPG','PDF');    /* DN */
        type  gpsCoordinateDN         dec(14,7);
        type  gpsSystemDN             enum('WGS-84','S-JTSK');
        type  hlavniPricinaDN         string(1,18);    /* AAND tab('CC_HlavniPricinaDN','HlavniPricinaDN'); */
        type  icDN                    num(1,12);
        type  identSouboruDN          long(1,9_999_999_999);
        type  jmenoDN                 string(1,36);
        type  jtskSouradniceDN        dec(14,7);
        type  kodDN                   num(17);              /* KodUtvaruPolicie+RokDN+CisloDN  */
        type  kodKrajePolicie         string(2);       /* AAND tab('CC_KrajPolicie','KrajPolicie'); */
        type  kodOkresu               string(4);      /* AAND tab('CC_Okres','KodOkresu'); */
        type  kodPojistiteleDN        num(4);
        type  kodPricinyDN            long(1,999);     /* AAND tab('CC_PricinaDN','KodPricinyDN'); */
        type  kodRizikovehoFaktoruDN  string(1,2);     /* AAND tab('CC_RizikovyFaktorDN','KodRizikovehoFaktoru'); */
        type  kodTextPojistiteleDN    string(1,30);
        type  kodUtvaruPolicie        num(6);          /* AAND tab('CC_UtvarPolicie','KodUtvaruPolicie'); */
        type  kodZdravPojistovnyDN    string();        /* AAND tabIfNum('CC_ZdravPojistovna','KodZdravPojistovny'); */
        type  krajPolicieNazev        string(1,20);    /* AAND tab('CC_KrajPolicie','KrajPolicieNazev'); */
        type  linkaDN                 string(1,20);
        type  linkaPoradiDN           string(1,10);
        type  mesicSTKDo              xdatetime('MM/yyyy');
        type  mistoNarozeniDN         string(1,60);
        type  mistoTextDN             string(1,255);
        type  mistoVydaniDN           string(1,50);
        type  mpzDN                   string(1,3);   /* hodnota není kontrolována oproti číselníku států */
        type  nalezenaShodaDN         long(1,255);
        type  nazevDN                 string(1,255);
        type  nazevFirmyDN            string(1,100);
        type  nazevPredmetuDN         string(1,255);
        type  nazevZvireteDN          string(1,255);
        type  obecDN                  string(1,50);
        type  okresDN                 string(1,36);
        type  opravneniDN             string(1,60);
        type  osobaTitulDN            string(1,36);
        type  osobniStavDN            string(1,30);
        type  oznSegmentuDN           string(1,5);
        type  pachatelDN              enum('Pachatel DN');
        type  planDatumVymazuDN       xdatetime('yyyy-MM-dd');
        type  platnostSTKDN           enum('platná','neplatná','nezjištěno');
        type  podtypParametruDN       string(1,50);    /* AAND tab('DNKL_PodtypParametru','PodtypParametru') */
        type  pohlaviDN               enum('M','Ž');
        type  pojisteniTextDN         enum('Doklad o pojištění nepředložen','Potvrzení nepředloženo',
        'Nepodléhá pojištění','Smlouva neuzavřena');
        type  pojistneHPDN            long(0,99_999);
        type  pokutaDN                string(1,7);
        type  pokutaZaplDN            enum('nezaplacena','zaplacena');
        type  popisRizikovehoFaktoruDN string(0,50);   /* AAND tab('CC_RizikovyFaktorDN','PopisRizikovehoFaktoru') */
        type  poradiPrilohyDN         long(1,99_999);
        type  poruseniTextDN          string(1,400);
        type  poruseniTrestDN         string(1,600);
        type  poskozenyDN             enum('Poškozený');
        type  praxeRizeniDN           long(0,99);
        type  pricinaDN               string(1,60);
        type  prijmeniDN              string(1,36);
        type  profeseDN               string(1,50);
        type  pscDN                   string(1,10);
        type  rcDN                    string(1,12);
        type  rokDN                   string(4);       /* AAND xdatetime('yyyy') */
        type  roleOsobyDN             string(1,20);    /* AAND tab('DNKL_RoleOsoby','RoleOsoby') */
        type  rozhodnutiDN            string(1,255);
        type  skodaTextDN             string(1,4000);
        type  spojDN                  string(1,50);
        type  spzDN                   string(1,20);
        type  statNazevDN             string(1,50);    /* AAND tab('CC_Stat','NazevStatuCZ') */
        type  stavBrzdPopisDN         string(1,100);  /* AAND tab('DN_StavBrzd','StavBrzdPopis') */
        type  subjektTextDN           string(1,255);
        type  telefonDN               string(1,28);
        type  typBrzdPopisDN          string(1,100);   /* AAND tab('DN_TypBrzd','TypBrzdPopis') */
        type  typDokladuDN            string(1,60);
        type  typKritickehoFaktoruDN  string(1,50);
        type  typParametruDN          string(1,50);    /* AAND tab('DNKL_TypParametru','TypParametru') */
        type  typPravidlaDN           string(1,50);
        type  typSegmentuDN           string(1,6);
        type  typSoupravyDN           string(1,40);
        type  typUcastnikaDN          string(1,40);
        type  typVozidlaDN            string(1,50);
        type  ucastnikRoleDN          enum('Pachatel DN','Poškozený');
        type  ucastnikTextDN          string(1,120);
        type  uliceDN                 string(1,50);
        type  utvarPolicieDN          string(1,70);
        type  vazbaDN                 string(1,400);
        type  verzeRozhraniDN         enum('2.3');
        type  vinDN                   string(1,26);
        type  vydalZKDN               string(1,255);
        type  vyhledanDleDN           string(1,40);
        type  vyhledanDlePopisDN      string(1,120);
        type  vyliceniTextDN          string(1,4000);
        type  zdravPojistovnaDN       string(1,100);
        type  znackaVozidlaDN         string(1,30);
        type  zpracovatelDN           string(1,60);
        type  zraneniDN               string(1);       /* AAND tab('DN_Zraneni','Zraneni') */
        type  zraneniTextDN           string(1,36);    /* AAND tab('DN_Zraneni','ZraneniPopis') */
    </xd:declaration>


    <SouborD3A                xd:script="ref Soubor" >
        <xd:mixed>
            <InfoZaznamPDN       xd:script="0..; ref InfoZaznamPDN" />
            <InfoPrilohaPDN      xd:script="0..; ref InfoPrilohaPDN" />
        </xd:mixed>
    </SouborD3A>

    <Adresa
            CisloOrientacni     ="? cisloDomuDN()"
            CisloPopisne        ="? cisloDomuDN()"
            Obec                ="? obecDN()"
            Okres               ="? okresDN()"
            PSC                 ="? pscDN()"
            Stat                ="? statNazevDN()"
            Telefon1            ="? telefonDN()"
            Telefon2            ="? telefonDN()"
            Ulice               ="? uliceDN()"
    />

    <Doklad
            CisloDokladu        ="? cisloDokladuDN()"
            DatumVydani         ="? datumDN()"
            MistoVydani         ="? mistoVydaniDN()"
            Opravneni           ="? opravneniDN()"
            StatVydani          ="? statNazevDN()"
            TypDokladu          ="  typDokladuDN()"
    />

    <Firma
            IC                  ="? icDN()"
            NazevFirmy          ="  nazevFirmyDN()">
        <Sidlo                 xd:script="0..1; ref Adresa" />
    </Firma>

    <InfoPojistitelDN
            KodPojistitele      ="? kodPojistiteleDN()"
            CisloSmlouvy        ="? cisloSmlouvy()"
            CisloORV            ="? cisloDokladu()"
            CisloTP             ="? cisloDokladu()"
            CisloZK             ="? cisloZKDN()"
            PoradiVozidla       ="? poradiVozidla()"
            CisloDokladuPojisteni="? cisloDokladuPojisteni()"
            SPZ                 ="? spzDN()"
            VIN                 ="? vinDN()"
            DuvodOdeslani       ="  duvodOdeslaniDN()"
    />

    <InfoPrilohaPDN
            CisloDN             ="  cisloDN()"
            Delka               ="  delkaPrilohyDN()"
            Format              ="  formatSouboruDN()"
            IdentSouboru        ="  identSouboruDN()"
            KodUtvaruPolicie    ="  kodUtvaruPolicie()"
            MD5                 ="  md5()"
            Poradi              ="  poradiPrilohyDN()"
            Repeat              ="? enum('A')"
            RokDN               ="  rokDN()"
            SeqRec              ="  recordSeq()"
            SeqSouboru          ="  fileSeq()"
    />

    <InfoZaznamPDN
            SeqRec              ="  recordSeq()"
            PlatnostOd          ="  xsDateTimeOd()">
        <AlternativniObraz  xd:script  = "occurs 0.."
                            PlatnostOd          ="  xsDateTimeOd()"
        />
        <xd:mixed>
            <Protokol             xd:script="1;    ref ProtokolDN" />
            <Rozhodnuti           xd:script="0..1; ref RozhodnutiDN" />
            <Vyliceni              xd:script="1">vyliceniTextDN()</Vyliceni>
            <ObjStranka           xd:script="1;    ref ObjStrankaDN" />
            <Vozidlo              xd:script="0..;  ref VozidloDN" />
            <Tramvaj              xd:script="0..;  ref TramvajDN" />
            <Trolejbus            xd:script="0..;  ref TrolejbusDN" />
            <Vlak                 xd:script="0..;  ref VlakDN" />
            <Povoz                xd:script="0..;  ref PovozDN" />
            <Predmet              xd:script="0..;  ref PredmetDN" />
            <Zvire                xd:script="0..;  ref ZvireDN" />
            <Ucastnik             xd:script="0..;  ref UcastnikDN" />
            <KlasifikaceDN        xd:script="0..;  ref KlasifikaceDN" />
            <InfoPojistitel       xd:script="0..;  ref InfoPojistitelDN" />
        </xd:mixed>
    </InfoZaznamPDN>

    <KlasifikaceDN
            TypParametru        ="  typParametruDN()"
            PodtypParametru     ="? podtypParametruDN()">
        <Osoba                    xd:script="0..1"
                                  Role                ="  roleOsobyDN()"
                                  refRC               ="? rcDN()"
                                  refSPZ              ="? spzDN()"
        />
        <KritickyFaktor           xd:script="0.."
                                  TypKritickehoFaktoru="? typKritickehoFaktoruDN()"
                                  TypPravidla         ="? typPravidlaDN()">
            <Detail               xd:script="0..">detailFaktorDN()</Detail>
        </KritickyFaktor>
    </KlasifikaceDN>

    <ObjStrankaDN
            CelkovaSkodaKc      ="? castkaKcDN()"
            CisloPopisne        ="? cisloDomuDN()"
            DatumCasDN          ="  datumCasDN()"
            DatumCasDoDN        ="? datumCasDN()"
            HlavniPricina       ="? hlavniPricinaDN()"
            KodOkresu           ="  kodOkresu()"
            Obec                ="? obecDN()"
            Pricina             ="? kodPricinyDN()"
            Ulice               ="? uliceDN()">
        <Misto                 xd:script="1">mistoTextDN()</Misto>
        <GPS                   xd:script="0..2"
                               Delka               ="  gpsCoordinateDN()"
                               Sirka               ="  gpsCoordinateDN()"
                               System              ="  gpsSystemDN()"
        />
        <RizikovyFaktor        xd:script="0.."
                               KodFaktoru          ="  kodRizikovehoFaktoruDN()"
        />
    </ObjStrankaDN>

    <Osoba
            DatumNarozeni       ="? datumDN()"
            Jmeno               ="? jmenoDN()"
            MistoNarozeni       ="? mistoNarozeniDN()"
            OsobniStav          ="? osobniStavDN()"
            Pohlavi             ="? pohlaviDN()"
            Prijmeni            ="  prijmeniDN()"
            Profese             ="? profeseDN()"
            RC                  ="? rcDN()"
            RodnePrijmeni       ="? prijmeniDN()"
            StatNarozeni        ="? statNazevDN()"
            StatniPrislusnost   ="? statNazevDN()"
            TitulPred           ="? osobaTitulDN()"
            TitulZa             ="? osobaTitulDN()">
        <Pobyt                 xd:script="0..1; ref Adresa" />
        <JinyPobyt             xd:script="0..1; ref Adresa" />
    </Osoba>

    <PovozDN
            DruhPovozu          ="  druhPovozuDN()"
            OznSegmentu         ="  oznSegmentuDN()">
        <Skoda                 xd:script="0..1; ref Skoda" />
        <JinaSkoda             xd:script="0..1; ref Skoda" />
        <Vlastnik              xd:script="0..1">subjektTextDN()</Vlastnik>
    </PovozDN>

    <PredmetDN
            DruhPredmetu        ="  druhPredmetuDN()"
            NazevPredmetu       ="  nazevPredmetuDN()"
            OznSegmentu         ="  oznSegmentuDN()">
        <Skoda                 xd:script="0..1; ref Skoda" />
        <Vlastnik              xd:script="0..1">subjektTextDN()</Vlastnik>
    </PredmetDN>

    <!-- Bude-li na vstupu mít KodDN na 7. a 8. místě hodnotu "21" nebo nižší, jedná se o starý formát,
         bude-li hodnota "22" nebo vyšší, jedná se o nový formát. Oba formáty se transformují
         do níže uvedené struktury tak, aby výsledně byl:
         RokDN num(4), tj. doplní se na začátek "20" a CisloDN num(7), tj. doplní se na začátek "00".
    -->
    <ProtokolDN
            CisloDN             ="  cisloDN()"
            CJ                  ="  cjRelacePolicieDN()"
            DatumCasOznameni    ="  datumCasDN()"
            KodUtvaruPolicie    ="  kodUtvaruPolicie()"
            NazevDN             ="  nazevDN()"
            RokDN               ="  rokDN()"
            Zpracovatel         ="  zpracovatelDN()"
    />

    <RozhodnutiDN
            DatumRozhodnuti     ="? datumDN()" >
        rozhodnutiDN()
    </RozhodnutiDN>

    <Skoda
            Kc                  ="? castkaKcDN()">
        ? skodaTextDN()
    </Skoda>

    <TramvajDN
            CisloDokladuPojisteni="? cisloDokladuPojisteniDN()"
            DruhVozidla         ="  druhVozidlaDN()"
            EvidCislo           ="? evidCisloDN()"
            KodPojistitele      ="? kodPojistiteleDN()"
            Linka               ="  linkaDN()"
            LinkaPoradi         ="? linkaPoradiDN()"
            OznSegmentu         ="  oznSegmentuDN()"
            PojisteniText       ="? pojisteniTextDN()"
            StavBrzd            ="? stavBrzdPopisDN()"
            TypBrzd             ="? typBrzdPopisDN()"
            TypVozidla          ="? typVozidlaDN()">
        <Skoda                 xd:script="0..1; ref Skoda" />
        <JinaSkoda             xd:script="0..1; ref Skoda" />
        <Vlastnik              xd:script="0..1">subjektTextDN()</Vlastnik>
    </TramvajDN>

    <TrolejbusDN
            CisloDokladuPojisteni="? cisloDokladuPojisteniDN()"
            DruhVozidla         ="  druhVozidlaDN()"
            EvidCislo           ="? evidCisloDN()"
            KodPojistitele      ="? kodPojistiteleDN()"
            Linka               ="  linkaDN()"
            LinkaPoradi         ="? linkaPoradiDN()"
            OznSegmentu         ="  oznSegmentuDN()"
            PojisteniText       ="? pojisteniTextDN()"
            StavBrzd            ="? stavBrzdPopisDN()"
            TypBrzd             ="? typBrzdPopisDN()"
            TypVozidla          ="? typVozidlaDN()">
        <Skoda                 xd:script="0..1; ref Skoda" />
        <JinaSkoda             xd:script="0..1; ref Skoda" />
        <Vlastnik              xd:script="0..1">subjektTextDN()</Vlastnik>
    </TrolejbusDN>

    <UcastnikDN
            AlkoholDech         ="? alkoholDechDN()"
            AlkoholKrev         ="? alkoholKrevDN()"
            OznSegmentu         ="  oznSegmentuDN()"
            Pachatel            ="? pachatelDN()"
            PokutaKc            ="? pokutaDN()"
            PokutaZapl          ="? pokutaZaplDN()"
            Poskozeny           ="? poskozenyDN()"
            PraxeRizeni         ="? praxeRizeniDN()"
            TypUcastnika        ="  typUcastnikaDN()"
            ZdravPojistovna     ="? zdravPojistovnaDN()"
            Zraneni             ="? zraneniDN()">
        <JinaSkoda             xd:script="0..1; ref Skoda" />
        <Osoba                 xd:script="0..1; ref Osoba" />
        <Firma                 xd:script="0..1; ref Firma" />
        <Doklad                xd:script="0..;  ref Doklad" />
        <PoruseniZakProvoz     xd:script="0..1">poruseniTextDN()</PoruseniZakProvoz>
        <PoruseniZakPrest      xd:script="0..1">poruseniTextDN()</PoruseniZakPrest>
        <PoruseniZakTrest      xd:script="0..1">poruseniTrestDN()</PoruseniZakTrest>
        <PoruseniZakOst        xd:script="0..1">poruseniTextDN()</PoruseniZakOst>
        <Vazba                 xd:script="0..1">vazbaDN()</Vazba>
    </UcastnikDN>

    <VlakDN
            DruhSoupravy        ="  druhSoupravyDN()"
            OznSegmentu         ="  oznSegmentuDN()"
            Spoj                ="? spojDN()"
            TypSoupravy         ="  typSoupravyDN()">
        <Skoda                 xd:script="0..1; ref Skoda" />
        <JinaSkoda             xd:script="0..1; ref Skoda" />
        <Vlastnik              xd:script="0..1">subjektTextDN()</Vlastnik>
    </VlakDN>

    <VozidloDN
            CisloDokladuPojisteni="? cisloDokladuPojisteniDN()"
            CisloZK             ="? cisloZKDN()"
            DatumUhradyHP       ="? datumDN()"
            DruhVozidla         ="  druhVozidlaDN()"
            KodPojistitele      ="? kodPojistiteleDN()"
            MesicSTKDo          ="? mesicSTKDo()"
            MistoUhradyHP       ="? mistoTextDN()"
            MPZ                 ="? mpzDN()"
            OznSegmentu         ="  oznSegmentuDN()"
            PlatnostSTK         ="? platnostSTKDN()"
            PlatnostZKDo        ="? datumCasDN()"
            PlatnostZKOd        ="? datumCasDN()"
            PojisteniText       ="? pojisteniTextDN()"
            PojistneHPKc        ="? pojistneHPDN()"
            RokVyroby           ="? rokDN()"
            SPZ                 ="? spzDN()"
            StavBrzd            ="? stavBrzdPopisDN()"
            TypBrzd             ="? typBrzdPopisDN()"
            TypVozidla          ="? typVozidlaDN()"
            VIN                 ="? vinDN()"
            VydalZK             ="? vydalZKDN()"
            ZnackaVozidla       ="? znackaVozidlaDN()">
        <Skoda                 xd:script="0..1; ref Skoda" />
        <JinaSkoda             xd:script="0..1; ref Skoda" />
        <Drzitel               xd:script="0..1">subjektTextDN()</Drzitel>
    </VozidloDN>

    <ZaznamPDN
            KrajPolicie         ="  kodKrajePolicie()"
            PlatnostOd          ="  datumCasDN()"
            SeqRec              ="? recordSeq()"
            Verze               ="? channelTypeVersion()">
        <xd:mixed>
            <Protokol              xd:script="1;    ref ProtokolDN" />
            <Rozhodnuti            xd:script="0..1; ref RozhodnutiDN" />
            <Vyliceni              xd:script="1">vyliceniTextDN()</Vyliceni>
            <ObjStranka            xd:script="1;    ref ObjStrankaDN" />
            <Vozidlo               xd:script="0..;  ref VozidloDN" />
            <Tramvaj               xd:script="0..;  ref TramvajDN" />
