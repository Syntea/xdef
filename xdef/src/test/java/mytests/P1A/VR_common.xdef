<?xml version="1.0" encoding="utf-8"?>
<xd:def xmlns:xd      ="http://www.xdef.org/xdef/4.0"
        impl-version  ="2018/04.2" impl-date="2023-08-23"
        xd:importLocal="GAM_type"
        xd:name       ="VR_common" >

 <Davka
         Kanal               ="  channelTypeCode()"
         Mode                ="  mode()"
         Odesilatel          ="  partnerCode()"
         OperationForm       ="? operationForm()"
         Prijemce            ="  partnerCode()"
         SeqDavky            ="  batchSeq()"
         SeqDavkyRef         ="? batchSeq()"
         UserField           ="? userField()"
         Vytvorena           ="  xsDateTimeVR()">
     <UserAttributes       xd:script="0..1"
            TransID          ="? transID"
     />
     <Soubor               xd:script="1.."
            DruhSouboru      ="  fileClass()"
            FormatSouboru    ="  fileFormat()"
            NazevSouboru     ="  fileName()"
            PocetZaznamu     ="  recordSeq()">
        <CheckSum
               Type          ="  checkSumType()"
               Value         ="  checkSumValue()"
        />
     </Soubor>
 </Davka>

 <Soubor
         Davka               ="  batchSeq()"
         ZeDne               ="  xsDateTimeVR() OR xsDateVR()"
 />
 <SouborOut
         Davka               ="  batchSeqOut()"
         ZeDne               ="  xsDateTimeVR() OR xsDateVR()"
 />
</xd:def>
